{
  "version": 2,
  "name": "wordly-storybook",
  "buildCommand": "npm run build:storybook",
  "devCommand": "npm run dev:storybook",
  "installCommand": "npm install",
  "outputDirectory": "storybook-static",
  "framework": null,
  "regions": ["sfo1"],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": true
    }
  },
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./src/stories/ ./.storybook/ ./tailwind.config.mjs ./src/app/globals.css"
}
